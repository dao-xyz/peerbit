{
  "name": "@dao-xyz/ipfs-log",
  "version": "5.5.37",
  "description": "Append-only log CRDT on IPFS",
  "author": "Haad",
  "license": "MIT",
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "!src/**/__tests__",
    "!lib/**/__tests__",
    "LICENSE"
  ],
  "publishConfig": {
