    "@dao-xyz/peerbit-io-utils": "^1.1.11",
    "json-stringify-deterministic": "^1.0.7",
    "multihashing-async": "^2.0.1",
    "p-do-whilst": "^1.1.0",
    "p-each-series": "^3.0.0",
    "p-map": "^5.5.0",
    "p-whilst": "^3.0.0"
  },
  "devDependencies": {
    "@dao-xyz/peerbit-keystore": "^0.0.21",
    "@dao-xyz/peerbit-test-utils": "^0.0.12",
    "@mapbox/node-pre-gyp": "^1.0.5",
    "assert": "^2.0.0",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "go-ipfs": "^0.16.0-rc1",
    "ipfs": "^0.65.0",
    "ipfsd-ctl": "^12.2.2",
    "is-node": "^1.0.2",
    "jsdoc": "^3.6.6",
    "json-loader": "~0.5.7",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-pre-gyp": "^0.13.0",
    "nyc": "^15.0.1",
    "orbit-db-benchmark-runner": "^1.0.3",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "contributors": [
    "haadcode",
    "aphelionz",
    "shamb0t",
    "thiagodelgado111",
    "mistakia",
    "satazor",
    "RichardLitt",
    "greenkeeperio-bot",
