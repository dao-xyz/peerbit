{
	"name": "@peerbit/program-react",
	"version": "0.4.0",
	"description": "React hooks for Peerbit programs",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!vitest",
		"!dist/vitest",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "vitest run",
		"lint": "aegir lint"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"dependencies": {
		"@peerbit/crypto": "workspace:*",
		"@peerbit/program": "workspace:*"
	},
	"devDependencies": {
		"@types/react": "^18.3.11",
		"@types/react-test-renderer": "^19.0.0",
		"@testing-library/react": "^16.1.0",
		"react-test-renderer": "^19.0.0"
	},
	"author": "dao.xyz",
	"license": "MIT"
}
