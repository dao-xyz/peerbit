{
  "name": "@dao-xyz/orbit-db-bdocstore",
  "version": "0.0.67",
  "description": "Document Store for orbit-db with binary ser/der",
  "type": "module",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | npx json > lib/cjs/package.json && echo '{\"type\":\"module\"} ' | npx json > lib/esm/package.json",
    "test": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.cjs --runInBand --forceExit",
    "test:unit": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.unit.cjs --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../jest.config.integration.cjs --runInBand --forceExit"
  },
  "author": "dao.xyz",
  "license": "MIT",
  "dependencies": {
    "@dao-xyz/borsh": "^2.1.4",
    "@dao-xyz/bpayload": "^0.0.5",
    "@dao-xyz/bquery": "^0.0.35",
    "@dao-xyz/orbit-db": "^0.29.10",
    "@dao-xyz/orbit-db-query-store": "^0.0.42",
    "@dao-xyz/orbit-db-store": "^0.0.16",
    "p-map": "~1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@dao-xyz-types/orbit-db": "^0.0.33",
    "@dao-xyz/peer-test-utils": "^0.0.24",
    "@dao-xyz/time": "^0.0.11",
    "ipfs": "^0.62.3"
  },
  "gitHead": "d10b89ef1a120a58e1a7e92aadcc2bb536a37e3e"
}
