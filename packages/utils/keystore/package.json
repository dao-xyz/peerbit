{
  "name": "@dao-xyz/peerbit-keystore",
  "version": "0.0.4",
  "description": "A local key manager for OrbitDB",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json",
    "postbuild": "echo '{\"type\":\"module\"} ' | node ../../../node_modules/.bin/json > lib/esm/package.json",
    "test": "node ../../../node_modules/.bin/jest test  -c  ../../../jest.config.ts --runInBand --forceExit",
    "test:unit": "node ../../../node_modules/.bin/jest test  -c  ../../../jest.config.unit.ts --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../../../jest.config.integration.ts --runInBand --forceExit"
  },
  "author": "Haad",
  "license": "MIT",
  "dependencies": {
    "@dao-xyz/borsh": "^3.3.0",
    "@dao-xyz/peerbit-borsh-utils": "^0.0.3",
    "@dao-xyz/peerbit-crypto": "^0.0.3",
    "@dao-xyz/peerbit-time": "^0.0.13",
    "elliptic": "^6.5.4",
    "level": "^8.0.0",
    "libp2p-crypto": "^0.19.4",
    "lru-cache": "^7.14.0",
    "mkdirp": "^0.5.5",
    "reachdown": "^1.1.0",
    "safe-buffer": "^5.2.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@dao-xyz/peerbit-test-utils": "^0.0.3",
    "@types/libsodium-wrappers": "^0.7.9",
    "is-node": "^1.0.2",
    "json-loader": "~0.5.7",
    "jsondown": "^1.0.0",
    "localstorage-down": "^0.6.7",
    "memdown": "^6.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "bugs": "https://github.com/dao-xyz/peerbit/issues",
  "homepage": "https://github.com/dao-xyz/peerbit",
  "contributors": [
    "adam-palazzo",
    "haadcode",
    "RichardLitt",
    "shamb0t",
    "thiagodelgado111",
    "willemneal"
  ],
  "localMaintainers": [
    "haad <haad@haja.io>",
    "shamb0t <shams@haja.io>",
    "hajamark <mark@haja.io>"
  ]
}
