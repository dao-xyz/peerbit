{
	"name": "@peerbit/cache",
	"version": "2.1.2",
	"description": "Simple cache",
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"homepage": "https://github.com/dao-xyz/peerbit",
	"bugs": "https://github.com/dao-xyz/peerbit/issues",
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test",
		"lint": "aegir lint"
	},
	"license": "MIT",
	"devDependencies": {
		"@peerbit/time": "2.0.8",
		"@types/yallist": "^4.0.4"
	},
	"dependencies": {
		"yallist": "^4.0.0"
	},
	"localMaintainers": [
		"dao.xyz"
	]
}
