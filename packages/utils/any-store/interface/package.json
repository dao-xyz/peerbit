{
	"name": "@peerbit/any-store-interface",
	"version": "1.0.0",
	"description": "Anystore interface",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		},
		"./messages": {
			"types": "./dist/src/messages.d.ts",
			"import": "./dist/src/messages.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"browser": {
		"./dist/src/store.js": "./dist/src/store.browser.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"homepage": "https://github.com/dao-xyz/peerbit",
	"bugs": "https://github.com/dao-xyz/peerbit/issues",
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build  --no-bundle"
	},
	"license": "MIT",
	"localMaintainers": [
		"dao.xyz"
	],
	"dependencies": {
		"@dao-xyz/borsh": "^5.2.4",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@types/uuid": "^10.0.0"
	}
}
