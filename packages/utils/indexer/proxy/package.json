{
  "name": "@peerbit/indexer-proxy",
  "version": "0.1.0",
  "description": "Proxy RPC contract and client for @peerbit/indexer-interface using borsh-rpc",
  "sideEffects": false,
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/e2e",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "build": "aegir build --no-bundle",
    "test": "aegir test --target node",
    "lint": "aegir lint"
  },
  "author": "dao.xyz",
  "license": "MIT",
  "dependencies": {
    "uuid": "^10.0.0",
    "@dao-xyz/borsh": "^5.2.3",
    "@dao-xyz/borsh-rpc": "^1.3.1",
    "@peerbit/indexer-interface": "^2.0.12"
  }
}