{
	"name": "peerbit",
	"version": "4.3.9",
	"description": "Peerbit client",
	"author": "dao.xyz",
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/e2e",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"engines": {
		"node": ">=18"
	},
	"browser": {
		"./dist/src/transports.js": "./dist/src/transports.browser.js"
	},
	"dependencies": {
		"@chainsafe/libp2p-noise": "^17.0.0",
		"@dao-xyz/borsh": "^5.2.3",
		"@libp2p/circuit-relay-v2": "^4.0.2",
		"@libp2p/crypto": "^5.1.10",
		"@libp2p/identify": "^4.0.2",
		"@libp2p/interface": "^3.0.0",
		"@libp2p/keychain": "^6.0.2",
		"@libp2p/tcp": "^11.0.2",
		"@libp2p/webrtc": "^6.0.3",
		"@libp2p/websockets": "^10.0.3",
		"@multiformats/multiaddr": "^13.0.1",
		"@peerbit/any-store": "workspace:*",
		"@peerbit/any-store-opfs": "workspace:*",
		"@peerbit/crypto": "workspace:*",
		"@peerbit/indexer-interface": "workspace:*",
		"@peerbit/logger": "workspace:*",
		"@peerbit/time": "workspace:*",
		"@peerbit/build-assets": "workspace:*",
		"@peerbit/keychain": "workspace:*",
		"@peerbit/indexer-simple": "workspace:*",
		"@peerbit/indexer-sqlite3": "workspace:*",
		"@sqlite.org/sqlite-wasm": "^3.50.4-build1",
		"@dao-xyz/datastore-level": "^11.1.0",
		"@chainsafe/libp2p-yamux": "^8.0.0",
		"level": "^10.0.0",
		"memory-level": "^3.1.0",
		"path-browserify": "^1.0.1",
		"libp2p": "^3.0.3",
		"p-queue": "^8.0.1",
		"uint8arrays": "^5.1.0",
		"@peerbit/program": "workspace:*",
		"@peerbit/blocks": "workspace:*",
		"@peerbit/pubsub": "workspace:*",
		"@peerbit/stream-interface": "workspace:*",
		"libsodium-wrappers": "^0.7.15"
	},
	"devDependencies": {
		"@types/libsodium-wrappers": "^0.7.14",
		"p-each-series": "^3.0.0",
		"p-map-series": "^3.0.0",
		"@types/path-browserify": "^1.0.2",
		"uuid": "^10.0.0"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test",
		"test:browser": "aegir clean && aegir test -t browser",
		"test:node": "aegir clean && aegir test -t node",
		"lint": "aegir lint"
	},
	"localMaintainers": [
		"dao.xyz"
	],
	"keywords": [
		"crdt",
		"database",
		"distributed",
		"decentralised",
		"distributed",
		"ipfs",
		"p2p",
		"peer-to-peer"
	]
}
