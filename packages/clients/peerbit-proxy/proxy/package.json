{
	"name": "@peerbit/proxy",
	"version": "5.4.5",
	"description": "Peerbit proxy host/server",
	"author": "dao.xyz",
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@dao-xyz/borsh": "^6.0.0",
		"@libp2p/interface": "^3.1.0",
		"@multiformats/multiaddr": "^13.0.1",
		"@peerbit/any-store": "workspace:*",
		"@peerbit/any-store-interface": "workspace:*",
		"@peerbit/blocks": "workspace:*",
		"@peerbit/blocks-interface": "workspace:*",
		"@peerbit/crypto": "workspace:*",
		"@peerbit/indexer-simple": "workspace:*",
		"@peerbit/indexer-interface": "workspace:*",
		"@peerbit/keychain": "workspace:*",
		"@peerbit/pubsub": "workspace:*",
		"@peerbit/pubsub-interface": "workspace:*",
		"@peerbit/stream": "workspace:*",
		"@peerbit/stream-interface": "workspace:*",
		"@peerbit/time": "workspace:*",
		"@peerbit/program": "workspace:*",
		"libp2p": "^3.1.0",
		"peerbit": "workspace:*",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@peerbit/test-utils": "workspace:*"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test --target node",
		"lint": "aegir lint"
	},
	"localMaintainers": [
		"dao.xyz"
	],
	"keywords": [
		"peerbit",
		"iframe"
	]
}
