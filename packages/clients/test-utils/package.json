{
	"name": "@peerbit/test-utils",
	"version": "2.3.19",
	"description": "Test utils for Peerbit",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/e2e",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		"./log-utils.js": {
			"types": "./dist/src/log-utils.d.ts",
			"import": "./dist/src/log-utils.js"
		},
		"./inmemory-libp2p.js": {
			"types": "./dist/src/inmemory-libp2p.d.ts",
			"import": "./dist/src/inmemory-libp2p.js"
		},
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"bench": "TS_NODE_PROJECT=./tsconfig.bench.json node --loader ts-node/esm ./benchmark/index.ts",
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test -t node",
		"lint": "aegir lint"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"engines": {
		"node": ">=16.15.1"
	},
	"author": "dao.xyz",
	"license": "MIT",
	"dependencies": {
		"@libp2p/interface": "^3.1.0",
		"@multiformats/multiaddr": "^13.0.1",
		"it-pushable": "^3.1.4",
		"libsodium-wrappers": "^0.7.15",
		"@peerbit/any-store": "workspace:*",
		"@peerbit/crypto": "workspace:*",
		"@peerbit/indexer-interface": "workspace:*",
		"@peerbit/libp2p-test-utils": "workspace:*",
		"peerbit": "workspace:*",
		"@chainsafe/libp2p-yamux": "^8.0.0",
		"@peerbit/blocks": "workspace:*",
		"@peerbit/keychain": "workspace:*",
		"@peerbit/program": "workspace:*",
		"@peerbit/pubsub": "workspace:*",
		"@peerbit/stream": "workspace:*",
		"libp2p": "^3.1.0",
		"tty-table": "^4.2.1"
	},
	"devDependencies": {
		"@types/libsodium-wrappers": "^0.7.14",
		"@peerbit/time": "workspace:*"
	},
	"localMaintainers": [
		"dao.xyz"
	]
}
