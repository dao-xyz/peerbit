    "@babel/preset-env": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "@mapbox/node-pre-gyp": "^1.0.5",
    "assert": "^2.0.0",
    "babel-loader": "~8.2.1",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "go-ipfs": "^0.10.0-rc1",
    "http-server": "^0.12.3",
    "ipfs": "^0.62.3",
    "ipfsd-ctl": "^10.0.3",
    "is-node": "^1.0.2",
    "jsdoc": "^3.6.6",
    "json-loader": "~0.5.7",
    "level-js": "^4.0.2",
    "levelup": "^4.4.0",
    "mocha": "^8.4.0",
    "mocha-headless-chrome": "^3.1.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-pre-gyp": "^0.13.0",
    "nyc": "^15.0.1",
    "orbit-db-benchmark-runner": "^1.0.3",
    "orbit-db-storage-adapter": "^0.5.3",
