{
  "name": "@dao-xyz/ipfs-log",
  "version": "5.5.7",
  "description": "Append-only log CRDT on IPFS",
  "author": "Haad",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "ipfs",
    "log",
    "crdts",
    "crdt"
  ],
  "dependencies": {
    "@dao-xyz/orbit-db-identity-provider": "^0.5.2",
    "json-stringify-deterministic": "^1.0.1",
    "multihashing-async": "^2.0.1",
    "orbit-db-io": "^1.0.2",
    "p-do-whilst": "^1.1.0",
    "p-each-series": "^2.1.0",
    "p-map": "^4.0.0",
    "p-whilst": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
