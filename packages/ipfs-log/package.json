    "env": "mocha",
    "ignore": [
      "lib/es5/",
      "docs/"
    ]
  },
  "contributors": [
    "haadcode",
    "aphelionz",
    "shamb0t",
    "thiagodelgado111",
    "mistakia",
    "satazor",
    "RichardLitt",
    "greenkeeperio-bot",
    "chrisdostert",
    "zachferland",
    "kaibakker",
    "dignifiedquire",
    "adam-palazzo"
