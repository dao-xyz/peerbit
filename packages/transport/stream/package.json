{
	"name": "@peerbit/stream",
	"version": "4.6.0",
	"description": "A building block for direct streaming protocols",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/e2e",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"bench": "TS_NODE_PROJECT=../../../tsconfig.json node --loader ts-node/esm ./benchmark/index.ts",
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test --target node",
		"lint": "aegir lint"
	},
	"engines": {
		"node": ">=16.15.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dao-xyz/peerbit.git"
	},
	"keywords": [
		"peerbit"
	],
	"author": "dao.xyz",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dao-xyz/peerbit/issues"
	},
	"homepage": "https://github.com/dao-xyz/peerbit#readme",
	"localMaintainers": [
		"dao.xyz"
	],
	"devDependencies": {
		"@peerbit/libp2p-test-utils": "workspace:*",
		"@peerbit/test-utils": "workspace:*",
		"@types/fast-fifo": "^1.0.2",
		"@types/sinon": "^10.0.20",
		"sinon": "^21.0.0",
		"@libp2p/crypto": "^5.1.10"
	},
	"dependencies": {
		"@chainsafe/libp2p-noise": "^17.0.0",
		"@chainsafe/libp2p-yamux": "^8.0.0",
		"@dao-xyz/borsh": "^6.0.0",
		"@libp2p/interface": "^3.1.0",
		"@libp2p/interface-internal": "^3.0.2",
		"@libp2p/tcp": "^11.0.2",
		"@libp2p/websockets": "^10.1.0",
		"@multiformats/multiaddr": "^13.0.1",
		"@multiformats/multiaddr-matcher": "^3.0.1",
		"abortable-iterator": "^5.0.1",
		"any-signal": "^4.1.0",
		"fast-fifo": "^1.3.2",
		"it-all": "^3.0.9",
		"it-length-prefixed": "^10.0.1",
		"it-pipe": "^3.0.1",
		"it-pushable": "^3.1.4",
		"libp2p": "^3.1.0",
		"p-defer": "^4.0.0",
		"p-queue": "^8.0.1",
		"uint8arraylist": "^2.4.8",
		"uint8arrays": "^5.1.0",
		"yallist": "^5.0.0",
		"@peerbit/cache": "workspace:*",
		"@peerbit/crypto": "workspace:*",
		"@peerbit/logger": "workspace:*",
		"@peerbit/stream-interface": "workspace:*",
		"@peerbit/time": "workspace:*"
	}
}
