{
	"name": "@peerbit/libp2p-test-utils",
	"version": "2.2.0",
	"description": "Test utils for Peerbit",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		"./inmemory-libp2p.js": {
			"types": "./dist/src/inmemory-libp2p.d.ts",
			"import": "./dist/src/inmemory-libp2p.js"
		},
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"engines": {
		"node": ">=16.15.1"
	},
	"author": "dao.xyz",
	"license": "MIT",
	"dependencies": {
		"@multiformats/multiaddr": "^13.0.1",
		"@libp2p/interface": "^3.1.0",
		"@libp2p/tcp": "^11.0.2",
		"@libp2p/webrtc": "^6.0.3",
		"@libp2p/websockets": "^10.1.0",
		"@libp2p/identify": "^4.0.2",
		"@libp2p/circuit-relay-v2": "^4.1.0",
		"@chainsafe/libp2p-noise": "^17.0.0",
		"@chainsafe/libp2p-yamux": "^8.0.0",
		"@peerbit/crypto": "workspace:*",
		"it-pushable": "^3.1.4",
		"libsodium-wrappers": "^0.7.15",
		"libp2p": "^3.1.0"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test -t node",
		"lint": "aegir lint"
	},
	"devDependencies": {
		"@types/libsodium-wrappers": "^0.7.14",
		"@peerbit/time": "workspace:*"
	},
	"localMaintainers": [
		"dao.xyz"
	]
}
