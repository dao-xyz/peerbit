{
	"name": "@peerbit/libp2p-test-utils",
	"version": "2.1.5",
	"description": "Test utils for Peerbit",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "peerbit",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"engines": {
		"node": ">=16.15.1"
	},
	"author": "dao.xyz",
	"license": "MIT",
	"dependencies": {
		"@libp2p/tcp": "^10.0.9",
		"@libp2p/webrtc": "^5.0.13",
		"@libp2p/websockets": "^9.0.8",
		"@libp2p/identify": "^3.0.8",
		"@libp2p/circuit-relay-v2": "^2.1.5",
		"libp2p": "^2.1.8"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test -t node",
		"lint": "aegir lint"
	},
	"devDependencies": {
		"@peerbit/time": "2.0.7"
	},
	"localMaintainers": [
		"dao.xyz"
	]
}
